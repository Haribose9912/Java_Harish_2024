package Day2;


public class removechar {
    public static void main(String[] args) {
     
     String s ="hariesh am here";
      StringBuilder sb= new StringBuilder();
     char rch='e';
     
     for(int i=0;i<s.length();i++){
       char chars =s.charAt(i);
       if(chars!=rch){
         sb.append(chars);
       }
     }System.out.println(sb);
     
  }
}