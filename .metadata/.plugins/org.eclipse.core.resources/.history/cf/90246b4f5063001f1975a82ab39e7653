package Day2;

public class FibonacciSeries {
	 public static void main(String[] args) {
	        int n = 10;
	        int a=0;
	        int b=1;
	        int c=0;
	        if (n<=0) {
	        	System.out.println("give numbers greater than zero");
	        }
	        else {
	        	for(int i=0;i<n;i++) {
	        		a=b;
	        		b=c;
	        		c=a+b;
	        	}
	        }System.out.println();
	        
	    }
	}


