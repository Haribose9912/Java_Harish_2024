package Selenium;

public class frame_handle {
		
	WebElement Frame_ele = drive.findndElement(By.xpath("//span[@class="name"]"))
	driver.switchTo().frame(int 2);
	driver.switchTo().frame(Frame_ele);
	driver.switchTo().frame("frame name");
	driver.switchTO().defaultConent();
	driver.switchTo().parentFrame();
	
	
}
